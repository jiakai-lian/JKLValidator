FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/JKLValidator.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/Kiwi.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "JKLValidator" -framework "Kiwi"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-JKLValidator_Tests
PODS_ROOT = ${SRCROOT}/Pods